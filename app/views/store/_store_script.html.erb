<script type='text/javascript'>
$(function() {
	$('.book_img').click(function() {
		$(this).parent().find(':submit').click();
	});
	
	$('.add_btn').click(function() {
		$(this).parent().parent().parent().parent().find('img').animate_from_to('.total_line');
	});
	
	$('#my-carousel').carousel();
})
</script>